{
  "manifest_version": 2,
  "name": "goodmarkR",
  "version": "1.0",
  "description": "This extension allows you to save bookmarks in an improved way!",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.1.4.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_title": "goodmarkR",
    "default_popup": "extension.html",
    "default_icon": "icon.png"
  }
}
